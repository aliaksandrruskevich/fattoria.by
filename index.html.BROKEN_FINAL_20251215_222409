    <script>
    // Исправленный поиск форм
    document.addEventListener('DOMContentLoaded', function() {
        // Ищем ВСЕ формы на странице
        const forms = document.querySelectorAll('form');
        forms.forEach((form, index) => {
            // Убираем стандартную отправку
            form.setAttribute('novalidate', 'true');
            form.addEventListener('submit', function(e) {
                e.preventDefault();
                const formData = new FormData(this);
                const data = Object.fromEntries(formData);
                // Добавляем информацию о странице
                data.source = window.location.pathname;
                data.timestamp = new Date().toISOString();
                // Показываем уведомление об отправке
                const submitBtn = form.querySelector('button[type="submit"]');
                if (submitBtn) {
                    const originalText = submitBtn.textContent;
                    submitBtn.textContent = 'Отправка...';
                    submitBtn.disabled = true;
                    setTimeout(() => {
                        submitBtn.textContent = originalText;
                        submitBtn.disabled = false;
                    }, 3000);
                }
                // Отправляем на сервер
                fetch('/api/submit-newbuilding.php', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(data)
                })
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network error');
                    }
                    return response.json();
                })
                .then(result => {
                    alert('Спасибо! Мы свяжемся с вами в ближайшее время.');
                    form.reset();
                })
                .catch(error => {
                    console.error('❌ Ошибка отправки:', error);
                    alert('Ошибка отправки. Пожалуйста, попробуйте еще раз или позвоните нам.');
                });
            });
        });
    });
    </script>
