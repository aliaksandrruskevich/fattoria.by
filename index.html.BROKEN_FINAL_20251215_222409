<script>
  ymaps.ready(init);
  function init(){
    var myMap = new ymaps.Map("map", {
      center: [53.9045, 27.5615], // Минск
      zoom: 10
    });
    // Загружаем объекты из API
    fetch('/api/properties.json')
      .then(response => response.json())
      .then(data => {
      loadingIndicator.style.display = 'none';
        if (Array.isArray(data)) {
          data.forEach((property, index) => {
            if (property.lat && property.lng) {
              // Используем реальные координаты из базы данных
              var lat = property.lat;
              var lng = property.lng;
            } else {
              // Генерируем случайные координаты вокруг Минска для демонстрации
              var lat = 53.9045 + (Math.random() - 0.5) * 0.1;
              var lng = 27.5615 + (Math.random() - 0.5) * 0.1;
            }
            var placemark = new ymaps.Placemark([lat, lng], {
              hintContent: property.title,
              balloonContent: `
                <div>
                  <h5>${property.title}</h5>
                  <p><strong>Цена:</strong> ${property.price} ${property.currency}</p>
                  <p><strong>Местоположение:</strong> ${property.location}</p>
                  <p><strong>Тип:</strong> ${property.type}</p>
                  <p>${property.description.substring(0, 100)}...</p>
                  <a href="/object?unid=${property.unid}" class="btn btn-warning btn-sm">Подробнее</a>
                </div>
              `
            });
            myMap.geoObjects.add(placemark);
          });
        }
      })
      .catch(error => {
        console.error('Ошибка загрузки объектов:', error);
        // Добавляем примеры маркеров
        var placemark1 = new ymaps.Placemark([53.9045, 27.5615], {
          hintContent: 'Квартира в центре',
          balloonContent: '3-комнатная квартира, 120 000 €'
        });
        var placemark2 = new ymaps.Placemark([53.9145, 27.5715], {
          hintContent: 'Загородный дом',
          balloonContent: 'Уютный дом с участком, 250 000 €'
        });
        myMap.geoObjects.add(placemark1);
        myMap.geoObjects.add(placemark2);
      });
  }
  // Функция для обработки клика по карточкам специальных предложений
  function openPropertyDetails(title, price, description, type, district, contactPhone) {
    // Создаем модальное окно с деталями объекта
    const modalHtml = `
      <div class="modal fade" id="propertyModal" tabindex="-1" aria-labelledby="propertyModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="propertyModalLabel">${title}</h5>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
              <div class="row">
                <div class="col-md-6">
                  <img width="800" height="600" src="https://images.unsplash.com/photo-1560448204-e02f11c3d0e2?ixlib=rb-4.0.3&auto=format&fit=crop&w=870&q=80"
                       class="img-fluid rounded mb-3" alt="${title}" loading="lazy">
                </div>
                <div class="col-md-6">
                  <h4 class="text-warning mb-3">${price}</h4>
                  <p><strong>Тип:</strong> ${type}</p>
                  <p><strong>Район:</strong> ${district}</p>
                  <p><strong>Описание:</strong> ${description}</p>
                  <div class="mt-4">
                    <a href="tel:+375296380053" class="btn btn-warning me-2">Заказать звонок</a>
                    <button class="btn btn-outline-primary" onclick="window.location.href='/properties'">Смотреть все объекты</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    `;
    // Удаляем существующее модальное окно, если оно есть
    const existingModal = document.getElementById('propertyModal');
    if (existingModal) {
      existingModal.remove();
    }
    // Добавляем новое модальное окно
    document.body.insertAdjacentHTML('beforeend', modalHtml);
    // Показываем модальное окно
    const modal = new bootstrap.Modal(document.getElementById('propertyModal'));
    modal.show();
  }
  // Функция для показа интереса к объекту
  function showInterest(propertyTitle) {
    // Закрываем модальное окно с деталями
    const propertyModal = bootstrap.Modal.getInstance(document.getElementById('propertyModal'));
    if (propertyModal) {
      propertyModal.hide();
    }
    // Показываем уведомление об успешной отправке
    showToast('Спасибо за интерес к объекту "' + propertyTitle + '"! Наш специалист свяжется с вами в ближайшее время.', 'success');
    // Здесь можно добавить код для отправки данных на сервер
  }
// Cookie Consent Script
document.addEventListener('DOMContentLoaded', function() {
  const cookieConsent = document.getElementById('cookieConsent');
  const acceptBtn = document.getElementById('acceptCookies');
  const declineBtn = document.getElementById('declineCookies');
  // Check if user has already made a choice
  if (!localStorage.getItem('cookieConsent')) {
    cookieConsent.style.display = 'block';
  }
  acceptBtn.addEventListener('click', function() {
    localStorage.setItem('cookieConsent', 'accepted');
    cookieConsent.style.display = 'none';
    // Enable analytics if needed
    gtag('consent', 'update', {
      'analytics_storage': 'granted'
    });
  });
  declineBtn.addEventListener('click', function() {
    localStorage.setItem('cookieConsent', 'declined');
    cookieConsent.style.display = 'none';
    // Disable analytics
    gtag('consent', 'update', {
      'analytics_storage': 'denied'
    });
  });
});
// Language Toggle Script
document.addEventListener('DOMContentLoaded', function() {
  const langToggle = document.getElementById('langToggle');
  if (langToggle) {
    langToggle.addEventListener('click', function() {
      // For now, just show an alert. In a real implementation, you'd switch languages.
      alert('Переключение на английский язык будет реализовано в будущем.');
    });
  }
});
// Dark Mode Toggle Script
document.addEventListener('DOMContentLoaded', function() {
  const darkModeToggle = document.getElementById("darkModeToggle"); if (!darkModeToggle) return;
  const body = document.body;
  const icon = darkModeToggle.querySelector('i');
  // Check for saved theme preference or default to light mode
  const currentTheme = localStorage.getItem('theme') || 'light';
  if (currentTheme === 'dark') {
    body.classList.add('dark-mode');
    icon.classList.remove('fa-moon');
    icon.classList.add('fa-sun');
  }
  darkModeToggle.addEventListener('click', function() {
    body.classList.toggle('dark-mode');
    const theme = body.classList.contains('dark-mode') ? 'dark' : 'light';
    localStorage.setItem('theme', theme);
    // Toggle icon
    if (theme === 'dark') {
      icon.classList.remove('fa-moon');
      icon.classList.add('fa-sun');
    } else {
      icon.classList.remove('fa-sun');
      icon.classList.add('fa-moon');
    }
  });
});
// Load Special Offers from API
document.addEventListener('DOMContentLoaded', function() {
  loadSpecialOffers();
});
function loadSpecialOffers() {
  const specialsContainer = document.getElementById('specials-container');
  const loadingIndicator = document.getElementById('specials-loading');
  const errorMessage = document.getElementById('specials-error');
  if (!specialsContainer) return;
  // Show loading
  loadingIndicator.style.display = 'block';
  errorMessage.style.display = 'none';
  specialsContainer.innerHTML = '';
  // Fetch properties from static JSON file
  fetch('/data/properties.json')
    .then(response => {
      if (!response.ok) {
        throw new Error(`HTTP error! status: ${response.status}`);
      }
      return response.json();
    })
    .then(data => {
      loadingIndicator.style.display = 'none';
      if (!data || data.length === 0) {
        specialsContainer.innerHTML = '<div class="col-12"><div class="alert alert-info text-center">Специальные предложения от Фаттории временно недоступны.</div></div>';
        return;
      }
      // Filter properties with special: true
      const specialOffers = data.filter(p => p.special === true);
      // Create special offer cards
      specialOffers.forEach((property, index) => {
        const col = document.createElement('div');
        col.className = 'col-md-4';
        col.setAttribute('data-aos', 'fade-up');
        col.setAttribute('data-aos-delay', (index * 200).toString());
        const card = document.createElement('div');
        card.className = 'special-card clickable-card';
        card.style.cursor = 'pointer';
        card.onclick = () => window.open(`properties.html`, "_blank");
        const imgContainer = document.createElement('div');
        imgContainer.className = 'position-relative';
        const img = document.createElement('img');
        img.src = property.photos && property.photos.length > 0 ? property.photos[0] : 'https://images.unsplash.com/photo-1560448204-e02f11c3d0e2?ixlib=rb-4.0.3&auto=format&fit=crop&w=870&q=80';
        img.className = 'special-img w-100';
        img.alt = property.title;
        img.loading = 'lazy';
        const badge = document.createElement('span');
        badge.className = 'special-badge';
        badge.textContent = getBadgeText(index);
        imgContainer.appendChild(img);
        imgContainer.appendChild(badge);
        const cardBody = document.createElement('div');
        cardBody.className = 'p-3';
        const title = document.createElement('h5');
        title.textContent = property.title;
        const description = document.createElement('p');
        description.textContent = property.description ? property.description.substring(0, 100) + '...' : 'Описание отсутствует';
        const price = document.createElement('p');
        price.className = 'text-warning fw-bold';
        price.textContent = formatPrice(property.price, property.currency);
        cardBody.appendChild(title);
        cardBody.appendChild(description);
        cardBody.appendChild(price);
        card.appendChild(imgContainer);
        card.appendChild(cardBody);
        col.appendChild(card);
        specialsContainer.appendChild(col);
      });
    })
    .catch(error => {
      console.error('Error loading special offers:', error);
      loadingIndicator.style.display = 'none';
      errorMessage.style.display = 'block';
    });
}
function getBadgeText(index) {
  const badges = ['Топ', 'Хит', 'Новинка'];
  return badges[index] || 'Спец';
}
function formatPrice(price, currency = 'USD') {
  if (!price) return 'Цена по запросу';
  const symbols = { 'USD': '$', 'EUR': '€', 'BYN': 'руб', 'RUB': 'р' };
  return new Intl.NumberFormat('ru-RU').format(price) + ' ' + (symbols[currency] || currency);
}
// Welcome Modal Script
document.addEventListener('DOMContentLoaded', function() {
  const welcomeModalEl = document.getElementById("welcomeModal"); if (!welcomeModalEl) return; const welcomeModal = new bootstrap.Modal(welcomeModalEl, {
    backdrop: 'static',
    keyboard: false
  });
  // Show modal on page load
  if (!localStorage.getItem('welcomeModalClosed')) {
    // Show modal after 2 seconds
    setTimeout(function() {
      welcomeModal.show();
    }, 2000);
  }
  // Handle modal close events
  document.getElementById('welcomeModal').addEventListener('hidden.bs.modal', function() {
    localStorage.setItem('welcomeModalClosed', 'true');
  });
  // Handle form submission
  document.getElementById('welcomeForm').addEventListener('submit', function(e) {
    e.preventDefault();
    const name = document.getElementById('modalName').value;
    const contact = document.getElementById('modalContact').value;
    // Prepare form data for Google Apps Script
    const formData = new FormData();
    formData.append('name', name);
    formData.append('contact', contact);
    formData.append('type', 'Приветственное модальное окно');
    formData.append('dateTime', new Date().toLocaleString());
    formData.append('userAgent', navigator.userAgent);
    formData.append('pageUrl', window.location.href);
    // Send to both Google Apps Script and email API
    const promise1 = fetch(scriptURL, {
      method: "POST",
      body: formData
    });
    const promise2 = fetch('/api/submit-newbuilding.php', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
      },
      body: JSON.stringify({
        name: name,
        contact: contact,
        source: 'welcome_modal'
      })
    });
    Promise.allSettled([promise1, promise2]).then(results => {
      const googleSuccess = results[0].status === 'fulfilled';
      const emailSuccess = results[1].status === 'fulfilled';
      if (googleSuccess || emailSuccess) {
        // Show success message
        const modalBody = document.querySelector('#welcomeModal .modal-body');
        modalBody.innerHTML = `
          <div class="text-center text-white">
            <h3 class="mb-3">Спасибо!</h3>
            <p class="mb-4">Мы свяжемся с вами в ближайшее время.</p>
            <button type="button" class="btn btn-warning btn-lg" data-bs-dismiss="modal">Закрыть</button>
          </div>
        `;
        // Mark as closed so it doesn't show again
        localStorage.setItem('welcomeModalClosed', 'true');
      } else {
        alert('Произошла ошибка при отправке формы. Попробуйте еще раз.');
      }
    }).catch(error => {
      console.error('Error:', error);
      alert('Произошла ошибка при отправке формы. Попробуйте еще раз.');
    });
  });
});
</script>
