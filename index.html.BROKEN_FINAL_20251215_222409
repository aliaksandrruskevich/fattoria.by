<script>
// УЛЬТРА изолированная версия - БЕЗ ФОРМЫ, только инпуты
function ultraCloseModal() {
    ultraResetModal();
}

    document.getElementById('ultraOfferScreen').style.display = 'none';
    document.getElementById('ultraFormScreen').style.display = 'block';
}

function ultraResetModal() {
    document.getElementById('ultraOfferScreen').style.display = 'block';
    document.getElementById('ultraFormScreen').style.display = 'none';
    document.getElementById('ultraSuccessScreen').style.display = 'none';
    document.getElementById('ultraName').value = '';
    document.getElementById('ultraPhone').value = '';
    document.getElementById('ultraEmail').value = '';
}

function ultraSubmitForm() {
    const name = document.getElementById('ultraName').value.trim();
    const phone = document.getElementById('ultraPhone').value.trim();
    const email = document.getElementById('ultraEmail').value.trim();
    
    if (!name || name.length < 2) {
        alert('Введите ваше имя');
        return;
    }
    
    if (!phone || phone.length < 5) {
        alert('Введите ваш телефон');
        return;
    }
    
    // Собираем данные ВРУЧНУЮ без FormData
    const data = {
        name: name,
        phone: phone,
        email: email,
        source: 'ultra_credit_modal',
        type: 'Кредит на новостройки',
        timestamp: new Date().toISOString()
    };
    
    // Отправляем ОДИН раз
    fetch('/api/submit-newbuilding.php', {
        method: 'POST',
        headers: {'Content-Type': 'application/json'},
        body: JSON.stringify(data)
    })
    .then(response => response.json())
    .then(result => {
        document.getElementById('ultraFormScreen').style.display = 'none';
        document.getElementById('ultraSuccessScreen').style.display = 'block';
    })
    .catch(error => {
        alert('Ошибка отправки. Позвоните: +375-29-638-00-53');
    });
}

</script>
